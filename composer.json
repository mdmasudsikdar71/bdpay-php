{
    "name": "mdmasudsikdar71/bdpay-php",
    "description": "PHP Library for Bangladesh Payment Gateways",
    "keywords": [
        "bdpay",
        "payment gateway",
        "api",
        "Bangladeshi payment gateway"
    ],
    "type": "library",
    "version": "1.0.0",
    "license": "proprietary",
    "homepage": "https://github.com/mdmasudsikdar71/bdpay-php",
    "authors": [
        {
            "name": "MD Masud Sikdar",
            "email": "masudsikdar85@gmail.com",
            "role": "Software Engineer"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "MDMasudSikdar\\BDPay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MDMasudSikdar\\BDPay\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
